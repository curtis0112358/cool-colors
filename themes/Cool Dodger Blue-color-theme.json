	"tokenColors": [
		/***********
		* Begin My Adds
		*************/
		{
			"name": "Gray",
			"scope": [
				"comment",
				"string.quoted.docstring.multi.python"
			],
			"settings": {
				"foreground": "#AAAAAA",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Yellow",
			"scope": [
				"source",
				"entity.name.tag",
				"support.type.property-name."
			],
			"settings": {
				"foreground": "#FFF07C",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Salmon",
			"scope": [
				"keyword.control.import"
			],
			"settings": {
				"foreground": "#FA8072",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Cool Green",
			"scope": [
				"keyword.operator",
				"keyword.operator.assignment",
				"storage.type",
			],
			"settings": {
				"foreground": "#00ff8c",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Pastel Fuscia",
			"scope": [
				"keyword.operator.logical",
				"keyword.operator.ternary.tf"
			],
			"settings": {
				"foreground": "#ff77ff",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Cyan",
			"scope": [
				"constant.numeric",
			],
			"settings": {
				"foreground": "#00ffff",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Orange",
			"scope": [
				"keyword.control.flow",
				"meta.function-call.arguments"
			],
			"settings": {
				"foreground": "#FF991C",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Darkish Blue",
			"scope": [
				"constant",
				"meta.lambda-function"
			],
			"settings": {
				"foreground": "#004cff",
				"fontStyle": "bold"
			}
		},
		{	"name": "Bright Green",
			"scope": [
				"meta.function-call",
				"keyword.declaration.resource.tf",
				"support.function.builtin"
			],
			"settings": {
				"foreground": "#00ff00",
				"fontStyle": "bold"
			}
		},
		{	"name": "White",
			"scope": [
				"meta.function.lambda.parameters",
				"meta.function-call.arguments.python",
				"string",
				"meta.locals.tf",
				"punctuation",
				"keyword.operator.arithmetic"
			],
			"settings": {
				"foreground": "#ffffff",
				"fontStyle": "bold"
			}
		},
	]
